    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-importer": "^0.2.5",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "*.{js,vue,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,less,scss,md}": [
      "prettier --write"
