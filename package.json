    "lint-staged": "^13.0.4",
    "naive-ui": "^2.34.3",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "stylelint": "^14.16.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-vue": "^1.4.0",
