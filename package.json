    "vuetify": "^3.0.4"
  },
  "devDependencies": {
    "@mdi/font": "^7.0.96",
    "@sicons/material": "^0.12.0",
    "@types/crypto-js": "^4.1.1",
    "@types/marked": "^4.0.7",
